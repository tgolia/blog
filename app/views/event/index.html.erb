<h1><%= @id %></h1>

<h1><%= @event.city %></h1>

<h1><%= @event.user.name %></h1>

<%= form_for(@event, url: edit_event_path(@id), method: :put) do |f| %>

  <div class="field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %> 
